        "oro/marketing": "2.0.*",
        "box/spout": "2.3.*",
        "brick/math": "0.5.*",
        "myclabs/deep-copy": "1.5.*",
        "leafo/scssphp": "0.6.3",
        "bower-asset/jquery-creditcardvalidator": "1.0",
